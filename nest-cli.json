{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "**/*.prisma",
        "outDir": "./dist"
      }
    ],
    "watchAssets": true
  },
  "generateOptions": {
    "spec": true,
    "flat": false
  },
  "monorepo": false,
  "root": "src"
}
